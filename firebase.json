{
    "hosting": [ {
      "target": "main",
      "public" : "dist/MyWebSite",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
         "rewrites": [ {
        "source": "**",
        "destination": "/index.html"
      } ]
    },
    {
      "target": "alt",
      "public" : "dist/MyWebSite",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
         "rewrites": [ {
        "source": "**",
        "destination": "/index.html"
      } ]
    }
  ]
}
